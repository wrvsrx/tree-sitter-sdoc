{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sdoc",
      "camelcase": "SDoc",
      "title": "sdoc",
      "scope": "source.sdoc",
      "file-types": [
        "sdoc"
      ],
      "injection-regex": "^sdoc$",
      "class-name": "TreeSitterSdoc"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Sdoc grammar for tree-sitter",
    "authors": [
      {
        "name": "wrvsrx",
        "email": "wrvsrx@outlook.com",
        "url": "https://github.com/wrvsrx"
      }
    ],
    "links": {
      "repository": "https://github.com/wrvsrx/tree-sitter-sdoc"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}